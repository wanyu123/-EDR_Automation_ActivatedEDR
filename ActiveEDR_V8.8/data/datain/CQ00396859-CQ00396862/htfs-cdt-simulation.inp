    HETRAN-PARAM INPUT-FILE= & 
    'HTFS-CDT-STRATING.EDR' SAV-INPUT=CREATE 
    FEEDS HOT=OPATO-ST COLD=CRUDEOIL 
    PRODUCTS HOT=OPA-COOL COLD=HOTCRUDE 
